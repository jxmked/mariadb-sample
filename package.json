{
  "name": "mariadb-sample",
  "version": "1.0.0",
  "description": "For static pages",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:tsc": "tsc -w -p tsconfig.json",
    "dev:sass": "sass --watch --style=expanded --embed-sources \"./src/styles/index.scss:./dist/index.css\"",
    "dev": "concurrently --raw \"npm run dev:tsc\" \"npm run dev:sass\"",
    "build:tsc": "tsc -p tsconfig.json --removeComments --sourceMap false",
    "build:sass": "sass --style=compressed --no-source-map \"./src/styles/index.scss:./dist/index.css\"",
    "build": "npm run build:tsc && npm run build:sass",
    "serve": "serve ."
  },
  "keywords": [],
  "author": "jxmked <jamc015lol@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "sass": "^1.56.1",
    "typescript": "^4.9.4"
  }
}
